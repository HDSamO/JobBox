@model IEnumerable<OnlineJobPortal.Models.keyword>

@foreach (var item in Model)
{
    <li>
        <label class="cb-container">
            <input name="keywordMeta" value="@item.meta" type="checkbox"><span class="text-small">@item.name</span><span class="checkmark"></span>
        </label>
        @Html.Action("GetAmountJobKeyword", "Job", new { id = item.id })
    </li>
}

