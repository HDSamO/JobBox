@model IEnumerable<OnlineJobPortal.Models.job_post>


<div class="@((long)@ViewBag.IndustryId == 1 ? "tab-pane fade show active" : "tab-pane fade")" id="tab-job-@(ViewBag.IndustryId)" role="tabpanel" aria-labelledby="tab-job-@(ViewBag.IndustryId)">
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-xl-3 col-lg-4 col-md-6 col-sm-6 col-12">
                <div class="card-grid-2 grid-bd-16 hover-up">
                    <div class="card-block-info pt-25">
                        <h6>
                            <a href="@ViewBag.meta/@item.meta/@item.id">
                                @{
                                    string jobTitle = item.job_title;
                                    if (jobTitle.Length > 25)
                                    {
                                        jobTitle = jobTitle.Substring(0, 25) + "...";
                                    }
                                }
                                @jobTitle
                            </a>
                        </h6>
                        <div class="mt-5">
                            @Html.Action("GetJobStatus", "Home", new { id = item.id })
                        </div>
                        <div class="mt-20 border-bottom pb-20">
                            @Html.Action("GetJobKeywords", "Home", new { id = item.id })
                        </div>
                        <div class="card-2-bottom mt-20">
                            <div class="row">
                                <div class="col-lg-7 col-md-7">
                                    <div class="d-flex">
                                        @Html.Action("GetJobCompany", "Home", new { id = item.id })
                                    </div>
                                </div>
                                <div class="col-lg-5 col-md-5 text-end"><span class="card-text-price">@item.job_salary vnd</span><span class="text-muted">/Hour</span></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        }
    </div>
</div>


